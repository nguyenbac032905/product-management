
doctype html
html 
    head 
        link(rel="preconnect" href="https://cdn.jsdelivr.net" crossorigin)
        link(rel="preconnect" href="https://cdnjs.cloudflare.com" crossorigin)
        link(rel="dns-prefetch" href="https://cdn.jsdelivr.net")
        title #{pageTitle}

        link(rel="preload" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css" as="style" onload="this.onload=null;this.rel='stylesheet'")
        link(rel="preload" href="https://cdn.jsdelivr.net/npm/@fortawesome/fontawesome-free@6.4.2/css/all.min.css" as="style" onload="this.onload=null;this.rel='stylesheet'")


        link(rel="preload" href="https://cdnjs.cloudflare.com/ajax/libs/viewerjs/1.11.7/viewer.css" as="style" onload="this.onload=null;this.rel='stylesheet'")
        link(rel="preload" href="https://unpkg.com/file-upload-with-preview/dist/style.css" as="style" onload="this.onload=null;this.rel='stylesheet'")
        link(rel="preload" href="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css" as="style" onload="this.onload=null;this.rel='stylesheet'")

        link(rel="stylesheet" href="/client/css/main.min.css")
        meta(name="google-site-verification" content="H2IMoh3XQLZaCrERNeJeBWwOjk2LVnaGepAGks4uVfI")
    body
        .layout
            include ../partials/header.pug
            main.main 
                block main
            include ../partials/footer.pug
        script(src="https://cdn.jsdelivr.net/npm/jquery@3.5.1/dist/jquery.min.js" defer)
        script(type="text/javascript" src="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js" defer)
        script(src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.bundle.min.js" defer)
        script(src="https://cdnjs.cloudflare.com/ajax/libs/viewerjs/1.11.7/viewer.min.js" defer)
        script(src="/socket.io/socket.io.js" defer)
        script(src="/js/socket.js" defer)
        script(src="/js/script.js" defer)
        script(src="/client/js/main.js" defer)
        block script